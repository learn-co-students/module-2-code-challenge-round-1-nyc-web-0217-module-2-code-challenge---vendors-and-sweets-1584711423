<h1>New Vendor_Sweet</h1>

<% if flash[:error] %>
    <% flash[:error].each do %>
    <p> <%= error %></p>
    <% end %>
<% end %>

<%= form_for(@vendor_sweet) do |f| %>
<%= f.label :vendor_id %>
<%= f.collection_select :vendor_id, @vendors, :id, :name %><br>

<%= f.label :sweet_id %>
<%= f.collection_select :sweet_id, @sweets, :id, :name %><br>

<%= f.label :price %>
<%= f.text_field :price %><br>
<%= f.submit %><br>
<% end %>

